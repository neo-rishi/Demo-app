<h2>Sign up</h2>
<% resource.build_profile %>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <table>
      <tr>
        <td><%=f.label :Email%></td>
        <td><%=f.email_field :email, autofocus: true, class: "form-control"%></td>
      </tr>
      <tr>
        <td><%=f.label :Password%></td>
        <td><%=f.password_field :password, autocomplete: "off", class: "form-control"%></td>
      </tr>
      <tr>
        <td><%=f.label :Conform_Password%></td>
        <td><%=f.password_field :password_confirmation, autocomplete:"off", class: "form-control"%></td>
      </tr>
       <tr>
          <td></td>
          <td colspan="2"><%=f.submit class:"btn btn-lg btn-primary btn-block"%></td>
      </tr>
    </table>
    <%= render "devise/shared/links" %>
  <%end%>
